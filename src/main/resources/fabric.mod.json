{
  "schemaVersion": 1,
  "id": "death_quota",
  "version": "${version}",
  "name": "Death Quota",
  "description": "Server-side Fabric mod that enforces a death quota before forcing players into spectator mode.",
  "authors": ["Bernardo"],
  "license": "MIT",
  "environment": "server",
  "entrypoints": {
    "server": [
      "net.deathquota.mod.DeathQuotaMod"
    ]
  },
  "mixins": [
    "death_quota.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.9",
    "fabric": "*",
    "minecraft": "1.21.5",
    "java": ">=21"
  }
}
